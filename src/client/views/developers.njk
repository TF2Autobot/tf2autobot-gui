	min-height: 960px;
}

.devs{
	margin-top: 50px;
}

.dev-info{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	margin-top: 50px;
}


.developer-img{
	width: 150px;
	height: 150px;
}

